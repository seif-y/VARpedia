<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wikispeak.controllers.ViewCreationsController">
   <children>
      <Text fx:id="creationDisplay" fill="#8a8484" layoutX="68.0" layoutY="352.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a Creation">
         <font>
            <Font size="26.0" />
         </font>
      </Text>
      <Button layoutX="288.0" layoutY="326.0" mnemonicParsing="false" onAction="#handlePlay" text="Play" />
      <Button layoutX="353.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleDelete" text="Delete" />
      <TextField layoutX="68.0" layoutY="58.0" prefHeight="31.0" prefWidth="371.0" promptText="Look for a creation" />
      <Button layoutX="452.0" layoutY="58.0" mnemonicParsing="false" text="Search" />
      <ListView fx:id="creationList" layoutX="64.0" layoutY="100.0" onMouseClicked="#handleSelectCreation" prefHeight="200.0" prefWidth="455.0" />
   </children>
</AnchorPane>
