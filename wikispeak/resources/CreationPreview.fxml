      <Button layoutX="176.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleScrap" text="Scrap Creation" />
      <Button layoutX="254.0" layoutY="290.0" mnemonicParsing="false" onAction="#handlePlay" text="Play/Pause" />
      <Button layoutX="194.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleBack" text="&lt;&lt;" />
